*{margin: 0;padding: 0;}
/*第一个主题内容*/
#index_content{width: 100%;height: 1329px; min-width: 1220px;/*background: darkorange;*/padding: 13px;box-sizing: border-box;}
#content_first{width: 1220px;height: 570px;margin: 0 auto;/*background: pink;*/position: relative;}
#content_left{width: 179px;height: 570px;background: white;border-radius: 5px 5px 5px 5px;box-shadow: 5px 5px 20px #808080;position: relative;}
/*全部商品分类*/
#commodity{height: 45px;text-align: center;line-height: 45px;color: #000000;font-size: 14px;font-weight: bold;}
hr{width: 155px;position: relative; left: 11px;}

#content_top{margin-left: 20px;width: 1030px;height: 330px;background: ghostwhite;position: absolute;right: 0px;top: 0;}
#content_down{margin-left: 20px;width: 1030px;height: 230px;background: gainsboro;position: absolute;right: 0px;bottom: 0px;}
#top_first{width: 830px;height: 330px;background: forestgreen;}
/*图片*/
#top_first img{width: 829px;height: 332px;top: -1px;cursor: pointer;}
#top_first #shadow{width: 829px;height: 30px;background: black;opacity: 0.1;position: absolute; bottom: 0;}
#top_second{width: 201px;height: 163px;background: firebrick;position: absolute;right: 0px;top: 0px;}
#top_second img{width: 201px;height: 163px;position: absolute;top: 0px;}
#top_three{width: 201px;height: 163px;background: darkcyan;position: absolute;right: 0px;bottom: 0px;}
#top_three img{width: 201px;height: 163px;position: absolute;top: 0px;}
#down_zero{width: 165px;height: 230px;background: green;}
#down_zero img{width: 166px;height: 230px;top: 0px;}
#down_first{width: 426px;height: 230px;background: gold;position: absolute;left: 169px;top: 0px;}
#down_first img{width: 426px;height: 230px;top: 0px;}
#down_second{width: 435px;height: 230px;background: silver;position: absolute; right: 0px;bottom: 0px;}
#down_second img{width: 435px;height: 230px;top: 0px;}

/*第二个主题内容*/
#content_second{width: 1220px;height: 480px;margin: 0 auto;/*background: skyblue;*/position: relative;}
#top_headline{width: 1220px;height: 75px;/*background: chartreuse;*/padding-top: 18px;box-sizing: border-box;}
#left_headline{width: 600px;height: 40px;/*background: darkcyan;*/line-height: 40px;float: left;}
#left_headline a{text-decoration: none;font-size: 18px;font-weight: bold;color: #333333;}
#headline_text{width: 540px;height: 40px;/*background: seashell;*/float: left;text-align: right;line-height: 40px;}
#right_headline{width: 70px;height: 40px;float: right;/*background: gold;*/position: relative;}
#right_headline img{width: 70px;height: 20px;position: absolute;top: 10px;}
#goods{width: 1220px;height: 200px;/*background: greenyellow;*/margin-top: -15px;}
#goods li{width: 201px;height: 200px;float: left;list-style: none;border: 1px solid rgba(0,0,0,0.15);margin-left: 0px;}
#goods #uls img{width: 200px;height: 200px;top: 0px; position: relative;margin-left: 1px;}
#headline_left{width: 605px;height: 30px;background: rgb(216,0,0);float: left;position: relative; top: 10px;text-align: center;line-height: 30px;color: white;}
#headline_right{width: 605px;height: 30px;background: rgb(255,0,88);float: right;position: relative;top: 10px;text-align: center;line-height: 30px;color: white;}
#goods_first{width: 605px;height: 167px;/*background: forestgreen;*/position: absolute;bottom: 0px;}
#goods_first #uls_first li{width: 199px;height: 165px;border: 1px solid #DCDCDC;float: left;list-style: none;cursor: pointer;}
#goods_first #uls_first img{width: 203px;height: 167px;float: left;position: absolute;top: 0px;margin-left: -1px;}
#goods_second{width: 618px;height: 167px;/*background: forestgreen;*/position: absolute;bottom: 0px;right: 0px;}
#goods_second #uls_second li{width: 191px;height: 165px;border: 1px solid #DCDCDC;float: left;list-style: none;position: relative; margin-left: 13px;left: 1px;cursor: pointer;}
#goods_second #uls_second img{width: 193px;height: 168px;float: right;position: absolute;top: -1px;left: -1px;}
